#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Workspace/money-keeper-backend/node_modules/.pnpm/tsup@8.0.2_ts-node@10.9.2_@types+node@20.12.8_typescript@5.4.5__typescript@5.4.5/node_modules/tsup/dist/node_modules:/mnt/d/Workspace/money-keeper-backend/node_modules/.pnpm/tsup@8.0.2_ts-node@10.9.2_@types+node@20.12.8_typescript@5.4.5__typescript@5.4.5/node_modules/tsup/node_modules:/mnt/d/Workspace/money-keeper-backend/node_modules/.pnpm/tsup@8.0.2_ts-node@10.9.2_@types+node@20.12.8_typescript@5.4.5__typescript@5.4.5/node_modules:/mnt/d/Workspace/money-keeper-backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Workspace/money-keeper-backend/node_modules/.pnpm/tsup@8.0.2_ts-node@10.9.2_@types+node@20.12.8_typescript@5.4.5__typescript@5.4.5/node_modules/tsup/dist/node_modules:/mnt/d/Workspace/money-keeper-backend/node_modules/.pnpm/tsup@8.0.2_ts-node@10.9.2_@types+node@20.12.8_typescript@5.4.5__typescript@5.4.5/node_modules/tsup/node_modules:/mnt/d/Workspace/money-keeper-backend/node_modules/.pnpm/tsup@8.0.2_ts-node@10.9.2_@types+node@20.12.8_typescript@5.4.5__typescript@5.4.5/node_modules:/mnt/d/Workspace/money-keeper-backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-default.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-default.js" "$@"
fi
